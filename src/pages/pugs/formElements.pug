                                        button.increment +
                                li.checkbox-item
                                    div.item-name дети
                                    div.item-count
                                        button.decrement -
                                        div.value 1
                                        button.increment +
                                li.checkbox-item
                                    div.item-name младенцы
                                    div.item-count
                                        button.decrement -
                                        div.value 0
                                        button.increment +
