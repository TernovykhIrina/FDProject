                        div.likebox
                            div.block-unliked
                                div.oval-unliked
                                    div.heart
                                    div.like-counter 2
                            div.block-liked
                                div.oval-liked
                                    div.heart
                                    div.like-counter 12
                    div.rate-buttons
                        label
                            span.label-text rate button
