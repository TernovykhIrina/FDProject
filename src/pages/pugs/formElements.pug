        div.container
            div.logo
            div.box1
                div.box1-colomn1
                    div.text-field-default
                        label
                            span.label-text text field
                            span.form-type-text default
                        input( type='text' placeholder="Email")
                    div.text-field-hover-focus
                        label
                            span.label-text text field
                            span.form-type-text hover / focus
                        input( type='text' placeholder="This is pretty awesome")
                    div.dropdown-1
                        label
                            span.label-text dropdown
                            span.form-type-text default
                        select.dropdown-select
